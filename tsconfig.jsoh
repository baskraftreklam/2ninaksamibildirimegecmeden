{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019"],
    "jsx": "react",
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "android", "ios"]
}
